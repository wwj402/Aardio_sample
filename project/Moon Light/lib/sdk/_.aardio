

class sdk{
	ctor(url){
		var v = handle( url )  
		if( !v ) return ; 
		this = v(url);
		var _getStreamInfo = this.getStreamInfo;
	};
	getStreamInfo = function(...){
		var ret, err = _getStreamInfo(...) 
		if( !ret ){
			return null, err; 
		}		
		var ok, err = this.checkResult(ret);
		if( !ok ){
			return null, err; 
		}
		return ret; 
	}
	checkResult = function(ret){
		if( !ret.website ){
			return null, "缺少网站名称"; 
		}
		if( !ret.roomId ){
			return null, "缺少直播间ID"; 
		}
		if( !ret.roomUrl ){
			return null, "缺少直播间地址"; 
		}					
		return true; 	
	}	
}

