namespace sdk{
 	handle = function(url){
		for( k, v in self){ 
			if( type(v) == type.class  ){
				if( v.canHandle(url) ){
					return v; 
				}
			}
		}
	}
	
	website = {}; 
	for( k, v in self){ 
		if( type(v) == type.class  ){
			var info = v.info;

			..table.push( website, {
				name = info.name;
				url = info.url;
				sdk = k;
			})
		}
	}	     	
}

